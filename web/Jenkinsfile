pipeline {
    agent any
    tools {
    maven 'mvncfg'
    }

    stages {
        stage ('Compile Stage') {

            steps {
                     
                    sh 'mvn -f /var/lib/jenkins/workspace/pipeline/web/pom.xml compile'
                
            }
        }

        stage ('Package Stage') {

            steps {
                    sh 'mvn -f /var/lib/jenkins/workspace/pipeline/web/pom.xml package'
                
            }
        }


        
    }
}
