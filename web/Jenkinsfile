pipeline {
    agent any
    tools {
    maven 'mvncfg'
    }

    stages {
      stage ('checkout code') {
         steps {
                git url: 'https://github.com/srinikolukula/prod.git'
               echo "url"
             }
           }


        stage ('Compile Stage') {

            steps {
                    
                   sh 'mvn -f /var/lib/jenkins/workspace/pipeline/web/pom.xml compile'
                
            }
        }

        stage ('Package Stage') {

            steps {
                   
                    sh 'mvn -f /var/lib/jenkins/workspace/pipeline/web/pom.xml package'
                
            }
        }


        
    }
}
