pipeline {
    agent any
    tools {
    maven 'mvncfg'
    }

    stages {
        stage ('Compile Stage') {

            steps {
                    
                   sh 'mvn /web/pom.xml compile'
                
            }
        }

        stage ('Package Stage') {

            steps {
                    sh 'mvn /web/package'
                
            }
        }


        
    }
}
